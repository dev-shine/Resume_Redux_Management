{
"appenders":{
  "rms-appender":{  "type": "file",
    "filename": "RMS.log",
    "maxLogSize": 10240,
    "backups": 3,
    "layout": {
        "type": "pattern",
        "pattern": "%d{dd/MM hh:mm} %-5p %m"
    }
  }
},
"categories": {
  "default": { "appenders": ["rms-appender" ], "level": "debug" }
}
}